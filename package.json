{
  "name": "@kinday/ramda",
  "version": "0.0.2",
  "description": "Kindayâ€™s Ramda wrapper with nifty tools",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "watch": "npm run transpile -- --watch",
    "coverage": "nyc -r=text -r=lcov npm test",
    "precoveralls": "npm run coverage",
    "coveralls": "coveralls < coverage/lcov.info",
    "test": "tape test/*.spec.js --require babel-register | tap-spec",
    "tdd": "chokidar **/*.js --command 'npm test' --initial --throttle=50 --polling",
    "transpile": "babel src --out-dir lib",
    "prepublish": "npm run transpile",
    "clean": "rimraf lib",
    "push": "git push --follow-tags",
    "postpublish": "npm-run-all clean push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kinday/ramda.git"
  },
  "keywords": [],
  "author": "Leonard Kinday <leonard@kinday.ru> (http://leonard.kinday.ru)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kinday/ramda/issues"
  },
  "homepage": "https://github.com/kinday/ramda#readme",
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-eslint": "^6.0.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "6.7.2",
    "chokidar-cli": "^1.2.0",
    "coveralls": "^2.11.8",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-require-path-exists": "^1.1.5",
    "npm-run-all": "^1.5.1",
    "nyc": "^6.0.0",
    "rimraf": "^2.5.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0"
  },
  "dependencies": {
    "ramda": "0.21"
  }
}
